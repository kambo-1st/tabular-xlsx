{
    "name": "kambo/tabular-xlsx",
    "description": "Tabular - an easy and effective way how to access XLSX data",
    "keywords": [
        "Kambo",
        "tabular"
    ],
    "homepage": "https://github.com/kambo-1st/tabular-xlsx",
    "license": "MIT",
    "authors": [
        {
            "name": "Bohuslav Å imek",
            "email": "bohuslav@simek.si",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-xmlwriter": "*",
        "ext-zip": "*"
    },
    "require-dev": {
        "fakerphp/faker": "^1.20",
        "phpunit/phpunit": "^9.0",
        "slevomat/coding-standard": "^8",
        "squizlabs/php_codesniffer": "^3",
        "symfony/console": "6.0.*"
    },
    "autoload": {
        "psr-4": {
            "Kambo\\Tabular\\XLSX\\": "src",
            "Kambo\\Tabular\\XLSX\\Test\\": "tests"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kambo\\Tabular\\XLSX\\Tests\\": "tests",
            "Kambo\\Tabular\\Tools\\Generator\\": "tools/generator/src/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "phpcbf": "vendor/bin/phpcbf",
        "phpcs": "vendor/bin/phpcs"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
